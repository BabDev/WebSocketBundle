{
    "name": "babdev/websocket-bundle",
    "type": "symfony-bundle",
    "description": "Bundle integrating the WebSocket Server library with Symfony",
    "keywords": [
        "symfony",
        "WAMP",
        "websocket",
        "websocket server"
    ],
    "license": "MIT",
    "require": {
        "php": "8.1.* || 8.2.*",
        "ext-pcntl": "*",
        "babdev/websocket-server": "0.1.*",
        "symfony/config": "^5.4 || ^6.2",
        "symfony/console": "^5.4 || ^6.2",
        "symfony/dependency-injection": "^5.4 || ^6.2",
        "symfony/deprecation-contracts": "^2.1 || ^3.0",
        "symfony/event-dispatcher": "^5.4 || ^6.2",
        "symfony/event-dispatcher-contracts": "^2.0 || ^3.0",
        "symfony/framework-bundle": "^5.4 || ^6.2",
        "symfony/http-kernel": "^5.4 || ^6.2",
        "symfony/routing": "^5.4 || ^6.2",
        "symfony/security-core": "^5.4 || ^6.2"
    },
    "require-dev": {
        "doctrine/dbal": "^3.4",
        "doctrine/doctrine-bundle": "^2.7",
        "fig/log-test": "^1.1",
        "matthiasnoback/symfony-config-test": "^4.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "1.10.13",
        "phpstan/phpstan-phpunit": "1.3.11",
        "phpstan/phpstan-symfony": "1.3.1",
        "phpunit/phpunit": "9.6.7",
        "psr/cache": "^1.0 || ^2.0 || ^3.0",
        "psr/container": "^1.0 || ^2.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "ratchet/rfc6455": "^0.3.1",
        "react/event-loop": "^1.3",
        "symfony/cache": "^5.4 || ^6.2",
        "symfony/http-foundation": "^5.4 || ^6.2",
        "symfony/phpunit-bridge": "^5.4 || ^6.2"
    },
    "conflict": {
        "doctrine/dbal": "<3.4 || >=4.0",
        "doctrine/doctrine-bundle": "<2.7 || >=3.0",
        "psr/cache": ">=4.0",
        "psr/container": ">=3.0",
        "psr/log": ">=4.0",
        "ratchet/rfc6455": "<0.3.1 || >=0.4",
        "react/event-loop": "<1.3 || >=2.0",
        "symfony/cache": "<5.4 || >=6.0 <6.2 || >=7.0",
        "symfony/http-foundation": "<5.4 || >=6.0 <6.2 || >=7.0"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\WebSocketBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\WebSocketBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev"
}
